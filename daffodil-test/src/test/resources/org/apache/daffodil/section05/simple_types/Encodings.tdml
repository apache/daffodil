      </tdml:documentPart>
    </tdml:document>
    <tdml:infoset>
      <tdml:dfdlInfoset>
        <ex:f422u001>
          <ls>65432</ls>
          <ms>1</ms>
          <value>123456</value>
        </ex:f422u001>
