    <xs:element name="dd7">
      <xs:complexType>
        <xs:sequence dfdl:separator=";">
          <xs:element name="branch" type="xs:string" dfdl:lengthKind="delimited" />
          <xs:choice dfdl:choiceDispatchKey="{ ./ex:branch }">
