          <xs:element name="descrip" type="xs:string"/>
        </xs:sequence>
      </xs:complexType>

      <xs:complexType name="blueSeq">
        <xs:sequence dfdl:separator="/" dfdl:separatorPosition="prefix" dfdl:separatorSuppressionPolicy="trailingEmptyStrict">
          <xs:element name="descrip" type="xs:string"/>
