<tdml:testSuite suiteName="PrefixedTests"
  description="Section 12 - lengthKind=prefixed" xmlns:tdml="http://www.ibm.com/xmlns/dfdl/testData"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
  xmlns:xs="http://www.w3.org/2001/XMLSchema" 
  xmlns:fn="http://www.w3.org/2005/xpath-functions"
  xmlns:ex="http://example.com"
  xmlns:tns="http://example.com"
  xmlns:dfdlx="http://www.ogf.org/dfdl/dfdl-1.0/extensions"
  defaultRoundTrip="onePass">

  <tdml:defineSchema name="lengthKindPrefixed-text.dfdl.xsd">

    <xs:include schemaLocation="org/apache/daffodil/xsd/DFDLGeneralFormat.dfdl.xsd" />

    <dfdl:format ref="ex:GeneralFormat"
      representation="text"
      encoding="UTF-8"
      binaryDecimalVirtualPoint="2"
      nilKind="literalValue"
      nilValue="%WSP+;"
      textBooleanTrueRep="TRUE"
      textBooleanFalseRep="FALSE"
      prefixIncludesPrefixLength="no" />

    <dfdl:defineFormat name="prefixedTxtByte">
      <dfdl:format lengthKind="prefixed" lengthUnits="bytes" prefixLengthType="ex:threeByteTextInt" />
    </dfdl:defineFormat>

    <dfdl:defineFormat name="prefixedBinByte">
      <dfdl:format lengthKind="prefixed" lengthUnits="bytes" prefixLengthType="ex:threeByteBinInt" />
    </dfdl:defineFormat>

    <dfdl:defineFormat name="prefixedTxtBits">
      <dfdl:format lengthKind="prefixed" lengthUnits="bits" prefixLengthType="ex:twentyFourBitsTextInt" />
    </dfdl:defineFormat>

    <dfdl:defineFormat name="prefixedBinBits">
      <dfdl:format lengthKind="prefixed" lengthUnits="bits" prefixLengthType="ex:twentyFourBitsBinInt" />
    </dfdl:defineFormat>

    <dfdl:defineFormat name="prefixedTxtChar">
      <dfdl:format lengthKind="prefixed" lengthUnits="characters" prefixLengthType="ex:threeCharTextInt" />
    </dfdl:defineFormat>


    <xs:simpleType name="threeByteTextInt" dfdl:representation="text" dfdl:lengthKind="explicit"
      dfdl:length="3" dfdl:lengthUnits="bytes">
      <xs:restriction base="xs:integer" /> 
    </xs:simpleType>

    <xs:simpleType name="threeByteBinInt" dfdl:representation="binary" dfdl:lengthKind="explicit"
      dfdl:length="3" dfdl:lengthUnits="bytes">
      <xs:restriction base="xs:integer" />
    </xs:simpleType>

    <xs:simpleType name="twentyFourBitsTextInt" dfdl:representation="text" dfdl:lengthKind="explicit"
      dfdl:length="24" dfdl:lengthUnits="bits">
      <xs:restriction base="xs:integer" /> 
    </xs:simpleType>

    <xs:simpleType name="twentyFourBitsBinInt" dfdl:representation="binary" dfdl:lengthKind="explicit"
      dfdl:length="24" dfdl:lengthUnits="bits">
      <xs:restriction base="xs:integer" />
    </xs:simpleType>

    <xs:simpleType name="threeCharTextInt" dfdl:representation="text" dfdl:lengthKind="explicit"
      dfdl:length="3" dfdl:lengthUnits="characters" dfdl:textNumberJustification="left"
      dfdl:textPadKind="padChar" dfdl:textTrimKind="padChar" dfdl:textNumberPadCharacter="X">
      <xs:restriction base="xs:integer" /> 
    </xs:simpleType>

    <xs:simpleType name="nByteTextInt" dfdl:representation="text" dfdl:lengthKind="prefixed"
      dfdl:prefixLengthType="ex:threeByteTextInt" dfdl:lengthUnits="bytes">
      <xs:restriction base="xs:integer" />
    </xs:simpleType>



    <xs:element name="pl_text_string_txt_bytes" type="xs:string" dfdl:ref="prefixedTxtByte"
      nillable="true" /> 
    <xs:element name="pl_text_string_txt_bits"  type="xs:string" dfdl:ref="prefixedTxtBits"
      nillable="true" />
    <xs:element name="pl_text_string_txt_chars" type="xs:string" dfdl:ref="prefixedTxtChar"
      nillable="true" dfdlx:parseUnparsePolicy="parseOnly" />

    <xs:element name="pl_text_string_txt_bytes_includes" type="xs:string" dfdl:ref="prefixedTxtByte"
      nillable="true" dfdl:prefixIncludesPrefixLength="yes" />
    <xs:element name="pl_text_string_txt_bits_includes" type="xs:string" dfdl:ref="prefixedTxtBits"
      nillable="true" dfdl:prefixIncludesPrefixLength="yes" />
    <xs:element name="pl_text_string_txt_chars_includes" type="xs:string" dfdl:ref="prefixedTxtChar"
      nillable="true"  dfdl:prefixIncludesPrefixLength="yes" dfdlx:parseUnparsePolicy="parseOnly" />

    <xs:element name="pl_text_string_bin_bytes" type="xs:string" dfdl:ref="prefixedBinByte"
      nillable="true" />
    <xs:element name="pl_text_string_bin_bits" type="xs:string" dfdl:ref="prefixedBinBits"
     nillable="true" />
 
    <xs:element name="pl_text_int_txt_bytes" type="xs:int" dfdl:ref="prefixedTxtByte" />
    <xs:element name="pl_text_int_txt_bits" type="xs:int" dfdl:ref="prefixedTxtBits" />
    <xs:element name="pl_text_int_txt_chars" type="xs:int" dfdl:ref="prefixedTxtChar"
      dfdlx:parseUnparsePolicy="parseOnly" />

    <xs:element name="pl_text_int_bin_bytes" type="xs:int" dfdl:ref="prefixedBinByte" />
    <xs:element name="pl_text_int_bin_bits" type="xs:int" dfdl:ref="prefixedBinBits" />


    <xs:element name="pl_text_int_txt_bytes_plbits" type="xs:int" dfdl:ref="prefixedTxtByte"
      dfdl:prefixLengthType="ex:twentyFourBitsTextInt" />
    <xs:element name="pl_text_int_txt_bytes_plchars" type="xs:int" dfdl:ref="prefixedTxtByte"
      dfdl:prefixLengthType="ex:threeCharTextInt" />

    <xs:element name="pl_text_int_txt_bits_plbytes" type="xs:int" dfdl:ref="prefixedTxtBits"
      dfdl:prefixLengthType="ex:threeByteTextInt" />
    <xs:element name="pl_text_int_txt_bits_plchars" type="xs:int" dfdl:ref="prefixedTxtBits"
      dfdl:prefixLengthType="ex:threeCharTextInt" />

    <xs:element name="pl_text_int_txt_chars_plbits" type="xs:int" dfdl:ref="prefixedTxtChar"
      dfdl:prefixLengthType="ex:twentyFourBitsTextInt"
      dfdlx:parseUnparsePolicy="parseOnly" />
    <xs:element name="pl_text_int_txt_chars_plbytes" type="xs:int" dfdl:ref="prefixedTxtChar"
      dfdl:prefixLengthType="ex:threeCharTextInt"
      dfdlx:parseUnparsePolicy="parseOnly" />

    <xs:element name="pl_text_int_bin_bytes_plbits" type="xs:int" dfdl:ref="prefixedBinByte"
      dfdl:prefixLengthType="ex:twentyFourBitsBinInt" />
    <xs:element name="pl_text_int_bin_bytes_plchars" type="xs:int" dfdl:ref="prefixedBinByte"
      dfdl:prefixLengthType="ex:threeCharTextInt" />
    <xs:element name="pl_text_int_bin_bits_plbytes" type="xs:int" dfdl:ref="prefixedBinBits"
      dfdl:prefixLengthType="ex:threeByteBinInt" />
    <xs:element name="pl_text_int_bin_bits_plchars" type="xs:int" dfdl:ref="prefixedBinBits"
      dfdl:prefixLengthType="ex:threeCharTextInt" />

    <xs:element name="pl_text_int_txt_bytes_includes" type="xs:int" dfdl:ref="prefixedTxtByte"
      dfdl:prefixIncludesPrefixLength="yes" />
    <xs:element name="pl_text_int_txt_bits_includes" type="xs:int" dfdl:ref="prefixedTxtBits"
      dfdl:prefixIncludesPrefixLength="yes" />
    <xs:element name="pl_text_int_txt_chars_includes" type="xs:int" dfdl:ref="prefixedTxtChar"
      dfdl:prefixIncludesPrefixLength="yes" dfdlx:parseUnparsePolicy="parseOnly" />

    <xs:element name="pl_text_int_bin_bytes_includes" type="xs:int" dfdl:ref="prefixedBinByte"
      dfdl:prefixIncludesPrefixLength="yes" />
    <xs:element name="pl_text_int_bin_bits_includes" type="xs:int" dfdl:ref="prefixedBinBits"
      dfdl:prefixIncludesPrefixLength="yes" />

    <xs:element name="pl_text_dec_txt_bytes" type="xs:decimal" dfdl:ref="prefixedTxtByte" />
    <xs:element name="pl_text_dec_txt_bits" type="xs:decimal" dfdl:ref="prefixedTxtBits" />
    <xs:element name="pl_text_dec_txt_chars" type="xs:decimal" dfdl:ref="prefixedTxtChar"
      dfdlx:parseUnparsePolicy="parseOnly" />

    <xs:element name="pl_text_dec_bin_bytes" type="xs:decimal" dfdl:ref="prefixedBinByte" />
    <xs:element name="pl_text_dec_bin_bits" type="xs:decimal" dfdl:ref="prefixedBinBits" />

    <xs:element name="pl_text_date_txt_bytes" type="xs:dateTime" dfdl:ref="prefixedTxtByte" />
    <xs:element name="pl_text_date_txt_bits" type="xs:dateTime" dfdl:ref="prefixedTxtBits" />
    <xs:element name="pl_text_date_txt_chars" type="xs:dateTime" dfdl:ref="prefixedTxtChar"
      dfdlx:parseUnparsePolicy="parseOnly" />

    <xs:element name="pl_text_date_bin_bytes" type="xs:dateTime" dfdl:ref="prefixedBinByte" />
    <xs:element name="pl_text_date_bin_bits" type="xs:dateTime" dfdl:ref="prefixedBinBits" />

    <xs:element name="pl_text_bool_txt_bytes" type="xs:boolean" dfdl:ref="prefixedTxtByte" />
    <xs:element name="pl_text_bool_txt_bits" type="xs:boolean" dfdl:ref="prefixedTxtBits" />
    <xs:element name="pl_text_bool_txt_chars" type="xs:boolean" dfdl:ref="prefixedTxtChar"
      dfdlx:parseUnparsePolicy="parseOnly" />

    <xs:element name="pl_text_bool_bin_bytes" type="xs:boolean" dfdl:ref="prefixedBinByte" />
    <xs:element name="pl_text_bool_bin_bits" type="xs:boolean" dfdl:ref="prefixedBinBits" />

    <xs:element name="pl_text_string_pl_txt_bytes" type="xs:string" dfdl:lengthKind="prefixed"
      dfdl:prefixLengthType="ex:nByteTextInt" dfdl:lengthUnits="bytes" />

    <xs:element name="pl_complex_bin_bytes">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="strings" dfdl:ref="prefixedBinByte">
            <xs:complexType>
              <xs:sequence dfdl:separator=",">
                <xs:element name="str" type="xs:string" maxOccurs="unbounded" dfdl:lengthKind="delimited" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="remaining" type="xs:string" dfdl:lengthKind="delimited" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>

    <xs:element name="pl_complex_bin_bits">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="strings" dfdl:ref="prefixedBinBits">
            <xs:complexType>
              <xs:sequence dfdl:separator=",">
                <xs:element name="str" type="xs:string" maxOccurs="unbounded" dfdl:lengthKind="delimited" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="remaining" type="xs:string" dfdl:lengthKind="delimited" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>

    <xs:element name="pl_complex_bin_bytes_suspension">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="lenValue" dfdl:ref="prefixedBinByte">
            <xs:complexType>
              <xs:sequence dfdl:separator=",">
                <xs:element name="len" type="xs:int" dfdl:lengthKind="delimited"
                  dfdl:outputValueCalc="{ dfdl:valueLength(../ex:value, 'bytes') + 3 }" />
                <xs:element name="value" type="xs:string" dfdl:lengthKind="explicit"
                  dfdl:length="{ ../ex:len }" dfdl:textStringJustification="right"
                  dfdl:textPadKind="padChar" dfdl:textTrimKind="padChar" dfdl:textStringPadCharacter="X" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="remaining" type="xs:string" dfdl:lengthKind="delimited" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>

    <xs:element name="pl_complex_bin_bytes_suspension_includes">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="lenValue" dfdl:ref="prefixedBinByte" dfdl:prefixIncludesPrefixLength="yes">
            <xs:complexType>
              <xs:sequence dfdl:separator=",">
                <xs:element name="len" type="xs:int" dfdl:lengthKind="delimited"
                  dfdl:outputValueCalc="{ dfdl:valueLength(../ex:value, 'bytes') + 3 }" />
                <xs:element name="value" type="xs:string" dfdl:lengthKind="explicit"
                  dfdl:length="{ ../ex:len }" dfdl:textStringJustification="right"
                  dfdl:textPadKind="padChar" dfdl:textTrimKind="padChar" dfdl:textStringPadCharacter="X" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="remaining" type="xs:string" dfdl:lengthKind="delimited" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>

  </tdml:defineSchema>

  <tdml:defineSchema name="lengthKindPrefixed-bin.dfdl.xsd">

    <xs:include schemaLocation="org/apache/daffodil/xsd/DFDLGeneralFormat.dfdl.xsd" />

    <dfdl:format ref="ex:GeneralFormat"
      representation="binary"
      encoding="UTF-8"
      binaryDecimalVirtualPoint="2"
      binaryBooleanTrueRep=""
      binaryBooleanFalseRep="0"
      binaryPackedSignCodes="C D F C"
      binaryNumberCheckPolicy="strict"
      calendarPatternKind="explicit"
      calendarPattern="MMddyyyyHHmmss"
      prefixIncludesPrefixLength="no" />

    <dfdl:defineFormat name="prefixedTxtByte">
      <dfdl:format lengthKind="prefixed" lengthUnits="bytes" prefixLengthType="ex:threeByteTextInt" />
    </dfdl:defineFormat>

    <dfdl:defineFormat name="prefixedBinByte">
      <dfdl:format lengthKind="prefixed" lengthUnits="bytes" prefixLengthType="ex:threeByteBinInt" />
    </dfdl:defineFormat>

    <dfdl:defineFormat name="prefixedTxtBits">
      <dfdl:format lengthKind="prefixed" lengthUnits="bits"  prefixLengthType="ex:twentyFourBitsTextInt" />
    </dfdl:defineFormat>

    <dfdl:defineFormat name="prefixedBinBits">
      <dfdl:format lengthKind="prefixed" lengthUnits="bits"  prefixLengthType="ex:twentyFourBitsBinInt" />
    </dfdl:defineFormat>

    <dfdl:defineFormat name="prefixedTxtChar">
      <dfdl:format lengthKind="prefixed" lengthUnits="characters" prefixLengthType="ex:threeCharTextInt" />
    </dfdl:defineFormat>


    <xs:simpleType name="threeByteTextInt" dfdl:representation="text" dfdl:lengthKind="explicit"
      dfdl:length="3" dfdl:lengthUnits="bytes">
      <xs:restriction base="xs:integer" /> 
    </xs:simpleType>

    <xs:simpleType name="threeByteBinInt" dfdl:representation="binary" dfdl:lengthKind="explicit"
      dfdl:length="3" dfdl:lengthUnits="bytes">
      <xs:restriction base="xs:integer" />
    </xs:simpleType>

    <xs:simpleType name="twentyFourBitsTextInt" dfdl:representation="text" dfdl:lengthKind="explicit"
      dfdl:length="24" dfdl:lengthUnits="bits">
      <xs:restriction base="xs:integer" /> 
    </xs:simpleType>

    <xs:simpleType name="twentyFourBitsBinInt" dfdl:representation="binary" dfdl:lengthKind="explicit"
      dfdl:length="24" dfdl:lengthUnits="bits">
      <xs:restriction base="xs:integer" />
    </xs:simpleType>

    <xs:simpleType name="threeCharTextInt" dfdl:representation="text" dfdl:lengthKind="explicit"
      dfdl:length="3" dfdl:lengthUnits="characters" dfdl:textNumberJustification="left"
      dfdl:textPadKind="padChar" dfdl:textTrimKind="padChar" dfdl:textNumberPadCharacter="X">
      <xs:restriction base="xs:integer" /> 
    </xs:simpleType>

    <xs:simpleType name="nByteTextInt" dfdl:representation="text" dfdl:lengthKind="prefixed"
      dfdl:prefixLengthType="ex:threeByteTextInt" dfdl:lengthUnits="bytes">
      <xs:restriction base="xs:integer" />
    </xs:simpleType>


    <xs:element name="pl_bin_int_txt_bytes" type="xs:int" dfdl:ref="prefixedTxtByte" />
    <xs:element name="pl_bin_int_txt_bits" type="xs:int" dfdl:ref="prefixedTxtBits" />

    <xs:element name="pl_bin_int_bin_bytes" type="xs:int" dfdl:ref="prefixedBinByte" />
    <xs:element name="pl_bin_int_bin_bits" type="xs:int" dfdl:ref="prefixedBinBits" />

    <xs:element name="pl_bin_int_txt_bytes_includes" type="xs:int" dfdl:ref="prefixedTxtByte"
      dfdl:prefixIncludesPrefixLength="yes" />
    <xs:element name="pl_bin_int_txt_bits_includes" type="xs:int" dfdl:ref="prefixedTxtBits"
      dfdl:prefixIncludesPrefixLength="yes" />

    <xs:element name="pl_bin_int_bin_bytes_includes" type="xs:int" dfdl:ref="prefixedBinByte"
      dfdl:prefixIncludesPrefixLength="yes" />
    <xs:element name="pl_bin_int_bin_bits_includes" type="xs:int" dfdl:ref="prefixedBinBits"
      dfdl:prefixIncludesPrefixLength="yes" />

    <xs:element name="pl_bin_int_bin_bytes_packed" type="xs:int" dfdl:ref="prefixedBinByte"
      dfdl:binaryNumberRep="packed" />
    <xs:element name="pl_bin_int_bin_bits_packed" type="xs:int" dfdl:ref="prefixedBinBits"
      dfdl:binaryNumberRep="packed" />

    <xs:element name="pl_bin_int_bin_bytes_bcd" type="xs:unsignedInt" dfdl:ref="prefixedBinByte"
      dfdl:binaryNumberRep="bcd" />
    <xs:element name="pl_bin_int_bin_bits_bcd" type="xs:unsignedInt" dfdl:ref="prefixedBinBits"
      dfdl:binaryNumberRep="bcd" />

    <xs:element name="pl_bin_int_bin_bytes_ibm4690" type="xs:int" dfdl:ref="prefixedBinByte"
      dfdl:binaryNumberRep="ibm4690Packed" />
    <xs:element name="pl_bin_int_bin_bits_ibm4690" type="xs:int" dfdl:ref="prefixedBinBits"
      dfdl:binaryNumberRep="ibm4690Packed" />

    <xs:element name="pl_bin_dec_txt_bytes" type="xs:decimal" dfdl:ref="prefixedTxtByte" />
    <xs:element name="pl_bin_dec_txt_bits" type="xs:decimal" dfdl:ref="prefixedTxtBits" />

    <xs:element name="pl_bin_dec_bin_bytes" type="xs:decimal" dfdl:ref="prefixedBinByte" />
    <xs:element name="pl_bin_dec_bin_bits" type="xs:decimal" dfdl:ref="prefixedBinBits" />

    <xs:element name="pl_bin_dec_bin_bytes_packed" type="xs:decimal" dfdl:ref="prefixedBinByte"
      dfdl:binaryNumberRep="packed" />
    <xs:element name="pl_bin_dec_bin_bits_packed" type="xs:decimal" dfdl:ref="prefixedBinBits"
      dfdl:binaryNumberRep="packed" />

    <xs:element name="pl_bin_dec_bin_bytes_bcd" type="xs:decimal" dfdl:ref="prefixedBinByte"
      dfdl:binaryNumberRep="bcd" />
    <xs:element name="pl_bin_dec_bin_bits_bcd" type="xs:decimal" dfdl:ref="prefixedBinBits"
      dfdl:binaryNumberRep="bcd" />

    <xs:element name="pl_bin_dec_bin_bytes_ibm4690" type="xs:decimal" dfdl:ref="prefixedBinByte"
      dfdl:binaryNumberRep="ibm4690Packed" />
    <xs:element name="pl_bin_dec_bin_bits_ibm4690" type="xs:decimal" dfdl:ref="prefixedBinBits"
      dfdl:binaryNumberRep="ibm4690Packed" />

    <xs:element name="pl_bin_hex_txt_bytes" type="xs:hexBinary" dfdl:ref="prefixedTxtByte" />
    <xs:element name="pl_bin_hex_txt_bits" type="xs:hexBinary" dfdl:ref="prefixedTxtBits" />

    <xs:element name="pl_bin_hex_bin_bytes" type="xs:hexBinary" dfdl:ref="prefixedBinByte" />
    <xs:element name="pl_bin_hex_bin_bits" type="xs:hexBinary" dfdl:ref="prefixedBinBits" />

    <xs:element name="pl_bin_bool_txt_bytes" type="xs:boolean" dfdl:ref="prefixedTxtByte" />
    <xs:element name="pl_bin_bool_txt_bits" type="xs:boolean" dfdl:ref="prefixedTxtBits" />

    <xs:element name="pl_bin_bool_bin_bytes" type="xs:boolean" dfdl:ref="prefixedBinByte" />
    <xs:element name="pl_bin_bool_bin_bits" type="xs:boolean" dfdl:ref="prefixedBinBits" />
    
    <xs:element name="pl_bin_date_bin_bytes_packed" type="xs:dateTime" dfdl:ref="prefixedBinByte"
      dfdl:binaryCalendarRep="packed" />
    <xs:element name="pl_bin_date_bin_bits_packed" type="xs:dateTime" dfdl:ref="prefixedBinBits"
      dfdl:binaryCalendarRep="packed" />

    <xs:element name="pl_bin_date_bin_bytes_bcd" type="xs:dateTime" dfdl:ref="prefixedBinByte"
      dfdl:binaryCalendarRep="bcd" />
    <xs:element name="pl_bin_date_bin_bits_bcd" type="xs:dateTime" dfdl:ref="prefixedBinBits"
      dfdl:binaryCalendarRep="bcd" />

    <xs:element name="pl_bin_date_bin_bytes_ibm4690" type="xs:dateTime" dfdl:ref="prefixedBinByte"
      dfdl:binaryCalendarRep="ibm4690Packed" />
    <xs:element name="pl_bin_date_bin_bits_ibm4690" type="xs:dateTime" dfdl:ref="prefixedBinBits"
      dfdl:binaryCalendarRep="ibm4690Packed" />

    <xs:simpleType name="plSlash1_string_prefix"
       dfdl:lengthKind="explicit" dfdl:length="4" dfdl:lengthUnits="bytes">
      <xs:restriction base="xs:unsignedByte" />
    </xs:simpleType>

    <xs:element name="plSlash1_data">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="string" type="xs:string" maxOccurs="unbounded"
            dfdl:lengthKind="prefixed" dfdl:prefixLengthType="ex:plSlash1_string_prefix"
            dfdl:lengthUnits="bytes" dfdl:alignment="4" dfdl:alignmentUnits="bytes" dfdl:fillByte="%NUL;" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>

  </tdml:defineSchema>

  <tdml:defineSchema name="lengthKindPrefixed-sde.dfdl.xsd">

    <xs:include schemaLocation="org/apache/daffodil/xsd/DFDLGeneralFormat.dfdl.xsd" />

    <dfdl:format ref="ex:GeneralFormat"
      prefixIncludesPrefixLength="no" />

    <xs:complexType name="prefixComplex">
      <xs:sequence>
        <xs:element name="foo" type="xs:int" dfdl:lengthKind="explicit"
          dfdl:length="3" dfdl:lengthUnits="bytes" />
      </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="prefixDelimited" dfdl:lengthKind="delimited">
      <xs:restriction base="xs:integer" /> 
    </xs:simpleType>

    <xs:simpleType name="prefixEndOfParent" dfdl:lengthKind="endOfParent">
      <xs:restriction base="xs:integer" /> 
    </xs:simpleType>

    <xs:simpleType name="prefixPattern" dfdl:lengthKind="pattern" dfdl:lengthPattern="[0-9]{1,2}">
      <xs:restriction base="xs:integer" /> 
    </xs:simpleType>

    <xs:simpleType name="prefixExpression" dfdl:lengthKind="explicit" dfdl:length="{ fn:count(.) }">
      <xs:restriction base="xs:integer" /> 
    </xs:simpleType>

    <xs:simpleType name="prefixOVC" dfdl:lengthKind="explicit" dfdl:length="2" dfdl:outputValueCalc="{ 5 }" >
