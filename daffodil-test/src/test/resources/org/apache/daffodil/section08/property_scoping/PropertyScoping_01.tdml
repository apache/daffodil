      <tdml:documentPart type="text">000000030000000600000001</tdml:documentPart>
    </tdml:document>
    <tdml:errors>
      <tdml:error>Short</tdml:error>
      <tdml:error>and</tdml:error>
      <tdml:error>long</tdml:error>
      <tdml:error>ref</tdml:error>
    </tdml:errors>
  </tdml:parserTestCase>
  
