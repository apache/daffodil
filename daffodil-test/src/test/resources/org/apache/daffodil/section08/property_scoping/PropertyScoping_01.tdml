    <tdml:infoset>
      <tdml:dfdlInfoset>
        <tns:easy>-1009</tns:easy>
      </tdml:dfdlInfoset>
    </tdml:infoset>
    <tdml:document>-1009</tdml:document>
  </tdml:unparserTestCase>

