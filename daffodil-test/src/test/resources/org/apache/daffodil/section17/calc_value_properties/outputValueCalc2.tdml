  <tdml:unparserTestCase name="ovc_w_runtime_group_sep" root="e3" model="ovc4" description="ovc test with textStandardGroupingSeparator as runtime valued property">
    <tdml:infoset>
      <tdml:dfdlInfoset>
        <ex:e3>
          <xi>X</xi>
          <yi>Y</yi>
          <zi>Z</zi>
          <z>12345.0</z>
        </ex:e3>
