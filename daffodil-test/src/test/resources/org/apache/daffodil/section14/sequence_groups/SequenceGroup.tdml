          <xs:sequence dfdl:hiddenGroupRef="ex:yCountGroup" />
          <xs:element name="y" type="xs:string" maxOccurs="unbounded"
            dfdl:occursCountKind="expression" dfdl:occursCount="{ ../ex:yCount }" />
