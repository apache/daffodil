                <xs:element name="count" type="xs:int" dfdl:inputValueCalc="{ fn:count(..) }"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    
    <xs:element name="stringLiteralFromString">
      <xs:complexType>
