          <xs:element name="str" type="xs:string" />
          <xs:element name="literal" type="xs:string"
            dfdl:lengthKind="implicit"
            dfdl:inputValueCalc="{ dfdl:stringLiteralFromString( ../ex:str) }" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>

