  </tdml:parserTestCase>

  <tdml:parserTestCase name="choiceBranchKeyRanges_oddLength_01"
    root="choiceBranchKeyRanges_oddLength_01" model="choiceBranchKeyRanges-Embedded.dfdl.xsd" description="Extensions - choiceBranchKeyRanges">

