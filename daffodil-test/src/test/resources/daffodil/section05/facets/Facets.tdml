					<data>5</data>
					<data>6</data>
					<data>7</data>
					<data>8</data>
					<data>9</data>
					<data>1</data>
					<data>2</data>
					<data>3</data>
					<data>4</data>
					<data>5</data>
				</basic>
			</tdml:dfdlInfoset>
		</tdml:infoset>
		
	
	</tdml:parserTestCase>

