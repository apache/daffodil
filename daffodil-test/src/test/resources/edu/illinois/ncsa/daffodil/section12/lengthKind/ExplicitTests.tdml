						dfdl:representation="binary" dfdl:lengthKind="implicit" />
					<xs:element name="address" dfdl:lengthKind="explicit"
						dfdl:length="{ ../ex:len }" dfdl:lengthUnits="bytes">
						<xs:complexType>
							<xs:sequence dfdl:sequenceKind="ordered">
