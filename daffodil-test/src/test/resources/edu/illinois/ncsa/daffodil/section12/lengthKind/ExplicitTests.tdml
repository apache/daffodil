						dfdl:length="10" dfdl:lengthUnits="characters" maxOccurs="3"
						minOccurs="3" dfdl:occursCountKind="fixed">
						<xs:complexType>
							<xs:sequence dfdl:sequenceKind="ordered">
								<xs:element name="A" type="xs:string"
