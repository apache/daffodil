      dfdl:lengthKind="explicit" dfdl:length="{ 21 }" />
    <xs:element name="nonNegIntBin" type="xs:nonNegativeInteger"
      dfdl:representation="binary" dfdl:lengthKind="explicit" dfdl:length="{ 16 }" />
