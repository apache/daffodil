        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <e1>
          <sibling1>
            <string7Bit>A</string7Bit>
            <hexBinary>A5</hexBinary>
          </sibling1>
          <sibling2>
            <string7Bit>5</string7Bit>
          </sibling2>
        </e1>
      </tdml:dfdlInfoset>
    </tdml:infoset>
  </tdml:parserTestCase>

