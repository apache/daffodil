    </xs:element>
    
    <xs:element name="e3" type="xs:string" dfdl:lengthKind="explicit" dfdl:length="1" />

    <xs:element name="ee1" type="xs:string" dfdl:encodingErrorPolicy="replace" dfdl:encoding="ASCII"
