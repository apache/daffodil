    <tdml:infoset>
      <tdml:dfdlInfoset>
        <e3>&#61695;</e3>
      </tdml:dfdlInfoset>
    </tdml:infoset>
  </tdml:parserTestCase>
   
  <tdml:parserTestCase name="xml_utf8_4byte_chars" root="e3"
